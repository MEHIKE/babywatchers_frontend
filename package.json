{
  "name": "babywatchers",
  "version": "0.0.1",
  "description": "Landing page for Babywatcher",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "watch:rsass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server --browser=chromium-browser",
    "start": "npm-run-all --parallel devserver watch:sass copy:css",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "copy:css": "copyfiles css/style.css src/",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css copy:css",
    "startreact": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "makeall": "npm-run-all build:css",
    "json-server": "json-server --watch db.json --port 5001",
    "dev": "concurrently \"npm run startreact\" \"npm run json-server\""
  },
  "author": "Rynno",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "concurrently": "^5.0.0",
    "json-server": "^0.15.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "i18next": "^17.0.18",
    "i18next-browser-languagedetector": "^4.0.0",
    "i18next-scanner": "^2.10.3",
    "i18next-xhr-backend": "^3.2.0",
    "lodash.merge": "^4.6.2",
    "moment": "^2.24.0",
    "react": "16.10.2",
    "react-autofocus": "^0.2.0",
    "react-dom": "16.10.2",
    "react-i18next": "^10.13.1",
    "react-localization": "^1.0.15",
    "react-moment": "^0.9.6",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-spinners": "^0.6.1",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.0",
    "uuid": "^3.3.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5001"
}
